019\", \"2020\",\n","#         \"2021\", \"2022\", \"2023\",\n","#         \"2024\", \"2025\", \"2026\",\n","#         \"2027\", \"2028\", \"2029\",\n","#         \"2030\", \"2031\", \"2032\",\n","#         \"2033\", \"2034\", \"2035\",\n","#         \"2036\", \"2037\", \"2038\",\n","#         \"2039\", \"2040\", \"2041\",\n","#         \"2042\", \"2043\", \"2044\",\n","#         \"2045\", \"2046\", \"2047\",\n","#         \"2048\", \"2049\"\n","#     ],\n","#     \"month\": [\n","#         \"01\", \"02\", \"03\",\n","#         \"04\", \"05\", \"06\",\n","#         \"07\", \"08\", \"09\",\n","#         \"10\", \"11\", \"12\"\n","#     ],\n","#     'area': [22.7, -105.7, 19.0, -101.5],\n","# }\n","\n","# client = cdsapi.Client()\n","# client.retrieve(dataset, request).download(\"eastward_wind.zip\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":17},"id":"lFYHSumAAtZ8","executionInfo":{"status":"ok","timestamp":1748275993121,"user_tz":360,"elapsed":18,"user":{"displayName":"Freddy Hernán Villota González","userId":"14478973492452153576"}},"outputId":"be5190fc-7643-40cf-d897-485ef8c7ee3f"},"execution_count":14,"outputs":[{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.HTML object>"],"text/html":["\n","            <style>\n","                .geemap-dark {\n","                    --jp-widgets-color: white;\n","                    --jp-widgets-label-color: white;\n","                    --jp-ui-font-color1: white;\n","                    --jp-layout-color2: #454545;\n","                    background-color: #383838;\n","                }\n","\n","                .geemap-dark .jupyter-button {\n","                    --jp-layout-color3: #383838;\n","                }\n","\n","                .geemap-colab {\n","                    background-color: var(--colab-primary-surface-color, white);\n","                }\n","\n","                .geemap-colab .jupyter-button {\n","                    --jp-layout-color3: var(--colab