.days_in_month\n","df_cmip6['solar_rad'] = df_cmip6['solar_rad'] * 3600 * 24 * dias_del_mes / 1e6  # → MJ/m²/mes\n","df_cmip6.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"TKT0C5hB0JLH","executionInfo":{"status":"ok","timestamp":1747614294270,"user_tz":360,"elapsed":56,"user":{"displayName":"Freddy Hernán Villota González","userId":"14478973492452153576"}},"outputId":"4c43a4b8-4f6d-49e3-910f-c24153bbfb0b"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.HTML object>"],"text/html":["\n","            <style>\n","                .geemap-dark {\n","                    --jp-widgets-color: white;\n","                    --jp-widgets-label-color: white;\n","                    --jp-ui-font-color1: white;\n","                    --jp-layout-color2: #454545;\n","                    background-color: #383838;\n","                }\n","\n","                .geemap-dark .jupyter-button {\n","                    --jp-layout-color3: #383838;\n","                }\n","\n","                .geemap-colab {\n","                    background-color: var(--colab-primary-surface-color, white);\n","                }\n","\n","                .geemap-colab .jupyter-button {\n","                    --jp-layout-color3: var(--colab-primary-surface-color, white);\n","                }\n","            </style>\n","            "]},"metadata":{}},{"output_type":"execute_result","data":{"text/plain":["                 date  precip_mm  year  month     temp_c    wind_u    wind_v  \\\n","0 2015-01-16 12:00:00   9.386863  2015      1  18.273834 -9.422890  0.641277   \n","1 2015-02-15 00:00:00  15.993876  2015      2  18.285797 -7.968413  0.566743   \n","2 2015-03-16 12:00:00  26.977810  2015      3  18.154694 -4.723524  0.570539   \n","3 2015-04-16 00:00:00  28.607992  2015      4  18.255768 -2.849622  0.169271   \n","4 2015-05-16 12:00:00  23.624590  2015      5  17.569214 -1.718866  0.157390   \n","